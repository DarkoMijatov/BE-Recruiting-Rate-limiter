<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>RateLimiter</PackageId>
        <Version>1.0.0</Version>
        <Authors>Your Name</Authors>
        <Description>Lightweight IP-based rate limiter middleware for ASP.NET Core 9, with optional per-endpoint limits.</Description>
        <PackageTags>rate limiting;middleware;aspnetcore</PackageTags>
        <RepositoryUrl>https://github.com/DarkoMijatov/BE-Recruiting-Rate-Limiter</RepositoryUrl>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>
    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
    </ItemGroup>
</Project>
